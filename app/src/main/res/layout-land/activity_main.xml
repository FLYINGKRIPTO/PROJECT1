<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ImageView
        android:id="@+id/rectangle1"
        android:layout_width="@dimen/rectangle_width"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle1"
         />
    <ImageView
        android:id="@+id/rectangle11"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/rectangle11marginStart"
        android:background="@drawable/rectangle1"
        />
    <ImageView
        android:id="@+id/rectangle2"
        android:layout_width="@dimen/rectangle_width"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle2"
        android:layout_marginStart="@dimen/marginStart1" />
    <ImageView
        android:id="@+id/rectangle22"
        android:layout_width="@dimen/rectangle_width"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle2"
        android:layout_marginStart="@dimen/rectangle22marginStart" />
    <ImageView
        android:id="@+id/rectangle3"
        android:layout_width="@dimen/rectangle_width"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle3"
        android:layout_marginStart="@dimen/marginStart2"
        />
<FrameLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/rectangle6"
        android:layout_width="@dimen/cardWidth"
        android:layout_height="@dimen/mgCardHeight"
        android:layout_marginStart="@dimen/CardMargin"
        android:layout_marginTop="@dimen/rectanglemarginTop"
        android:src="@drawable/rectangle8" />

    <ImageView
        android:layout_width="@dimen/muzikgallerywidth"
        android:layout_height="wrap_content"
        android:src="@drawable/muzikgallery"
        android:layout_marginTop="@dimen/muzikgallerymargintop"
        android:layout_marginStart="@dimen/InstrumentMargin1" />
</FrameLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/nopath"
        android:layout_marginStart="@dimen/playermarginstart" />
    <TextView
        android:id="@+id/musicgallery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iconmoon"
        android:layout_marginTop="@dimen/marginTopmg"
        android:text="@string/music_gallery"
        android:textSize="@dimen/TextSizePrimary"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginStart="@dimen/textMarginStart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/address1"
        android:autoLink="map"
        android:onClick="process"
        android:layout_width="@dimen/addWidth"
        android:layout_height="wrap_content"
        android:textSize="@dimen/TextSizeSecondary"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/addMarginTop"
        android:text="@string/address1"
        android:textColor="@color/black"
        android:alpha="0.8"
        android:layout_marginStart="@dimen/textMarginStart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/address2"
        android:onClick="process"
        android:layout_width="@dimen/addressWidth"
        android:layout_height="wrap_content"
        android:autoLink="map"
        android:textSize="@dimen/TextSizeSecondary"
        android:textStyle="bold"
        android:layout_marginTop="230dp"
        android:text="@string/address2"
        android:textColor="@color/black"
        android:alpha="0.8"
        android:layout_marginStart="@dimen/textMarginStart" />
     <TextView
         android:fontFamily="@font/iconmoon"
         android:id="@+id/contact"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="@dimen/contactmarginTop"
         android:text="@string/contact"
         android:textColor="@color/black"
         android:textSize="@dimen/TextSizeSecondary"
         android:layout_marginStart="@dimen/textMarginStart"
         android:autoLink="phone"
         />
     <ImageView
         android:layout_width="@dimen/rectWidth"
         android:layout_marginTop="@dimen/rectmargintop"
         android:background="@drawable/rectangle7"
         android:layout_height="@dimen/marginTopmg"
         android:layout_marginStart="@dimen/marginStartrect" />
    <ImageView
        android:src="@drawable/guitar"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/landrow1margintop"
        android:layout_marginStart="@dimen/column1marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/guitar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/guitar"
        android:layout_marginTop="@dimen/landrow1margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column2marginstart" />
    <ImageView
        android:src="@drawable/piano"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/row2margintop"
        android:layout_marginStart="@dimen/column1marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/keyboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/keyboard"
        android:layout_marginTop="@dimen/landrow2margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column2marginstart" />
    <ImageView
        android:src="@drawable/drum"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/landrow3margintop"
        android:layout_marginStart="@dimen/column1marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/drums"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/drums"
        android:layout_marginTop="@dimen/landrow3margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column2marginstart" />
    <ImageView
        android:src="@drawable/harmonium"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/landrow1margintop"
        android:layout_marginStart="@dimen/column3marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/harmonium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/harmonium"
        android:layout_marginTop="@dimen/landrow1margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column4marginstart" />
    <ImageView
        android:src="@drawable/tabla"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/row2margintop"
        android:layout_marginStart="@dimen/column3marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/tabla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tabla"
        android:layout_marginTop="@dimen/landrow2margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column4marginstart" />
    <ImageView
        android:src="@drawable/flute"
        android:tint="@color/white"
        android:layout_width="@dimen/instrumentIcon"
        android:layout_height="@dimen/instrumentIcon"
        android:layout_marginTop="@dimen/landrow3margintop"
        android:layout_marginStart="@dimen/column3marginstart" />
    <TextView
        android:fontFamily="@font/iconmoon"
        android:id="@+id/flute"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/flute"
        android:layout_marginTop="@dimen/landrow3margintoptext"
        android:textColor="@color/white"
        android:textSize="@dimen/TextSizeSecondary"
        android:layout_marginStart="@dimen/column4marginstart" />
</RelativeLayout>
